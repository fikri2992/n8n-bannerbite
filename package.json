{
  "name": "n8n-bannerbite",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint nodes credentials src --ext .ts",
    "lint:fix": "eslint nodes credentials src --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "env:example": "cp .env.example .env",
    "env:check": "ts-node src/utils/check-env.ts"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BannerbiteApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bannerbite/BannerbiteNode.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^18.19.110",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.18.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.14.1",
    "ts-node": "^10.9.2",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "node-fetch": "^2.6.7"
  },
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": "",
  "license": "MIT",
  "description": "Custom n8n node with API key authentication and dynamic form generation"
}
